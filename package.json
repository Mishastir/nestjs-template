{
  "name": "--service_name---backend",
  "private": true,
  "version": "1.0.0",
  "description": "--service_name-- backend repository",
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --tsc --watch --exec 'node --require dotenv/config .' -p tsconfig.build.json",
    "start:debug": "nest start --debug --watch --tsc",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "sh scripts/migrations/local/migration-create.sh",
    "migration:gen": "sh scripts/migrations/local/migration-gen.sh",
    "migration:up:dev": "sh scripts/migrations/local/migration-up-dev.sh",
    "migration:down:dev": "sh scripts/migrations/local/migration-down-dev.sh",
    "migration:up": "sh scripts/migrations/prod/migration-up.sh",
    "migration:down": "sh scripts/migrations/prod/migration-down.sh",
    "migration:show": "sh scripts/migrations/prod/migration-show.sh",
    "seed": "echo SEED",
    "prepare": "husky install",
    "check-circular": "madge --circular src/main.ts"
  },
  "lint-staged": {
    "*.ts": "npm run format --"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.438.0",
    "@aws-sdk/hash-node": "^3.374.0",
    "@aws-sdk/s3-request-presigner": "^3.438.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/throttler": "^5.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pino": "^8.16.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.7",
    "@types/eslint": "^8.44.6",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "madge": "^6.1.0",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
